  "bugs": {
    "url": "https://github.com/zhao-jun/anydoc/issues"
  },
  "homepage": "https://github.com/zhao-jun/anydoc#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.0"
  },
  "dependencies": {
    "art-template": "^4.12.2",
    "chalk": "^2.4.1",
